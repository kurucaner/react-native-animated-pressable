{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "es6",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": "./src"
  }
}
